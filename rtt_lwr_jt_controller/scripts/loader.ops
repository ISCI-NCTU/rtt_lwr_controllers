import("rtt_ros")
ros.import("rtt_rosclock")
ros.import("rtt_state_publisher")
ros.import("rtt_lwr_abstract")
loadService("Deployer","ros_helper")

var string robot_name = ros_helper.getRobotName()

ros.import("rtt_lwr_jt_controller")
loadComponent("jt","lwr::JtATIController")

setActivity("jt",0.001,LowestPriority,ORO_SCHED_OTHER)

connectPeers("jt",robot_name)

jt.configure()
