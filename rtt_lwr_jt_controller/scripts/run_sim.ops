import("rtt_ros")
ros.import("rtt_lwr_jt_controller")
loadComponent("jt","lwr::JtATIController")
setActivity("jt",0.001,LowestPriority,ORO_SCHED_RT)
connectPeers("jt","lwr_sim")
stream("jt.ft_data_in", ros.comm.topic("ft_sensor/wrench"))
jt.use_sim_clock = true
jt.connect_all_ports_at_startup = false

jt.configure()
jt.connectAllPorts("lwr_sim")
jt.setDamping(0.01)
