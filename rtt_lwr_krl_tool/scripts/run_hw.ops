import("rtt_ros")

ros.import("rtt_lwr_krl_tool")
loadComponent("krl_tool","lwr::KRLTool")
setActivity("krl_tool",0.01,10,ORO_SCHED_OTHER)
connectPeers("krl_tool","lwr")
connect("krl_tool.toKRL","lwr.toKRL",ConnPolicy())
connect("krl_tool.fromKRL","lwr.fromKRL",ConnPolicy())
connect("krl_tool.JointImpedanceCommand","lwr.JointImpedanceCommand",ConnPolicy())
krl_tool.configure()

