// Start LWR SIMULATION
import("rtt_ros")

ros.import("rtt_lwr_krl_tool")
loadComponent("krl_tool","lwr::KRLTool")
setActivity("krl_tool",0.01,10,ORO_SCHED_OTHER)
connectPeers("krl_tool","lwr_sim")
connect("krl_tool.toKRL","lwr_sim.toKRL",ConnPolicy())
connect("krl_tool.fromKRL","lwr_sim.fromKRL",ConnPolicy())
connect("krl_tool.JointImpedanceCommand","lwr_sim.JointImpedanceCommand",ConnPolicy())
krl_tool.configure()


